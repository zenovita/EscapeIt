<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EndGame">
   <children>
      <Label layoutX="285.0" layoutY="181.0" prefHeight="67.0" prefWidth="150.0" text="Time Survived:">
         <font>
            <Font name="Comic Sans MS" size="18.0" />
         </font>
      </Label>
      <Button fx:id="playAgain" layoutX="270.0" layoutY="459.0" mnemonicParsing="false" onAction="#playAgainListener" prefHeight="81.0" prefWidth="150.0" text="Play Again">
         <font>
            <Font name="Comic Sans MS" size="21.0" />
         </font>
      </Button>
      <Label fx:id="point" layoutX="636.0" layoutY="271.0" prefHeight="67.0" prefWidth="121.0" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font></Label>
      <Label fx:id="timeSurvived" layoutX="627.0" layoutY="181.0" prefHeight="67.0" prefWidth="121.0" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font></Label>
      <Label layoutX="314.0" layoutY="271.0" prefHeight="67.0" prefWidth="121.0" text="Point:">
         <font>
            <Font name="Comic Sans MS" size="22.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="588.0" layoutY="459.0" mnemonicParsing="false" onAction="#backButtonListener" prefHeight="81.0" prefWidth="150.0" text="Back to Menu">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
